{
  "name": "@qytera/bruno-xray-plugin",
  "version": "0.0.1",
  "description": "A plugin for uploading Bruno API test results to Xray.",
  "main": "index.js",
  "scripts": {
    "test": "node --import ./test/loader.js ./test/run-unit-tests.ts",
    "build": "tsc --project tsconfig-build.json && shx cp README.md package.json package-lock.json .oclifrc.json dist/ && shx mkdir dist/src/bin && shx cp src/bin/run.* dist/src/bin",
    "lint": "npx eslint src/**/*.ts test/**/*.ts"
  },
  "author": "dev@qytera.de",
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/Qytera-Gmbh/bruno-xray-plugin.git",
    "type": "git"
  },
  "keywords": [
    "bruno",
    "xray",
    "upload",
    "results"
  ],
  "dependencies": {
    "@oclif/core": "^4.0.32",
    "ansi-colors": "^4.1.3",
    "csv-parse": "^5.6.0"
  },
  "bin": {
    "bruno-xray-plugin": "src/bin/run.js"
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@usebruno/cli": "^1.38.3",
    "eslint": "^9.17.0",
    "eslint-plugin-perfectionist": "^4.6.0",
    "oclif": "^4.17.10",
    "prettier": "^3.4.2",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1"
  }
}
