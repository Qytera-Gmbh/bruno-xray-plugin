{
  "name": "@qytera/bruno-xray-reporter",
  "version": "0.0.1",
  "description": "A plugin for uploading Bruno API test results to Xray.",
  "main": "index.js",
  "scripts": {
    "test": "node --import ./test/loader.js ./test/run-unit-tests.ts",
    "build": "tsc --project tsconfig-build.json && shx cp package.json .oclifrc.json dist/ && shx mkdir dist/src/bin && shx cp src/bin/run.* dist/src/bin",
    "lint": "npx eslint src/**/*.ts test/**/*.ts"
  },
  "author": "dev@qytera.de",
  "license": "MIT",
  "keywords": [
    "bruno",
    "xray",
    "upload",
    "results"
  ],
  "dependencies": {
    "@oclif/core": "^4.0.32",
    "ansi-colors": "^4.1.3",
    "csv-parse": "^5.6.0"
  },
  "bin": {
    "bruno-xray-reporter": "./src/bin/run.js"
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@usebruno/cli": "^1.38.3",
    "eslint": "^9.17.0",
    "eslint-plugin-perfectionist": "^4.6.0",
    "oclif": "^4.15.28",
    "prettier": "^3.4.2",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.19.1"
  }
}
